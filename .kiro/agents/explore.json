{
  "name": "explore",
  "description": "Fast, read-only codebase exploration specialist for finding files, searching code, and answering questions about codebases",
  "prompt": "file://../prompts/explore-prompt.md",
  "model": "claude-haiku-4-5",
  "tools": [
    "read",
    "shell"
  ],
  "allowedTools": [
    "read",
    "shell"
  ],
  "toolsSettings": {
    "shell": {
      "allowedCommands": [
        "ls",
        "find",
        "cat",
        "head",
        "tail",
        "wc",
        "git status",
        "git log",
        "git diff",
        "git branch",
        "git show",
        "tree",
        "file",
        "stat"
      ]
    }
  },
  "resources": [
    "file://README.md",
    "file://package.json",
    "file://tsconfig.json",
    "file://Cargo.toml",
    "file://pyproject.toml",
    "file://go.mod"
  ]
}
